#Vector of linear regression coefficients to predict the 1st var. (1 column) by the remaining columns
import numpy as np
from numpy import linalg
dataSet = np.array([[2,60], [2,50], [2, 75]])
myData = np.copy(dataSet)
Y = np.copy(myData[:,0])
myData[:,0]=1
regrVec = ((linalg.inv(myData.T.dot(myData))).dot(myData.T)).dot(Y)
